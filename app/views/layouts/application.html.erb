<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag    "application","http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/smoothness/jquery-ui.css", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "ckeditor/ckeditor.js" %>
  <%= favicon_link_tag %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/navbar' %>
  <div class="container">
    <%= render 'layouts/message' if flash.present? %>
    <%= yield %>
    <%= render 'layouts/footer' %>
  </div> <!-- /container -->
  <%= debug(params) if Rails.env.development? %>
  <%= debug(session) if Rails.env.development? %>
  <%= yield :scripts %>
</body>
</html>
